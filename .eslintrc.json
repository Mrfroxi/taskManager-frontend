{
  "extends": ["react-app", "react-app/jest", "prettier"],
  "plugins": ["react-hooks"],
  "rules":{
    "@typescript-eslint/indent": ["error", "tab"],
    "array-bracket-spacing": ["error", "always"],
    "no-useless-escape": "off",
    "default-case": "off",
    "semi":"warn",
    "react-hooks/rules-of-hooks": "off",
    "react-hooks/exhaustive-deps": "off", 
  }
}
